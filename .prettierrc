{
  "semi": false,
  "trailingComma": "all",
  "singleQuote": false,
  "printWidth": 88,
  "tabWidth": 2,
  "useTabs": false,
  "quoteProps": "as-needed",
  "proseWrap": "always",
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "importOrder": [
    "^(next/(.*)$)|^(next$)",
    "^(react/(.*)$)|^(react$)",
    "<THIRD_PARTY_MODULES>",
    "^@/types/(.*)$",
    "^@/lib/(.*)$",
    "^@/schemas/(.*)$",
    "^@/actions/(.*)$",
    "^@/data/(.*)$",
    "^@/components/ui/(.*)$",
    "^@/components/(.*)$|^components/(.*)$",
    "^@/styles/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "endOfLine": "lf"
}
